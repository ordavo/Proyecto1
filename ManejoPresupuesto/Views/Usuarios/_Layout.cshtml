<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ManejoPresupuesto</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ManejoPresupuesto.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Custom.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Cuentas/Index.cshtml.css" asp-append-version="true" />
    <link rel="stylesheet" href="<link rel=" stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css" href="ManejoPresupuestos.styles.css" asp-append-version="true" />
    



</head>
<body style="margin: 0;">
    <div style="display: flex; justify-content: center; align-items: center; height: 40vh">
        <div >
            <h1 style="text-align: center; color: blue">Camilo Alvarez</h1>
            <h2 style="text-align: center;">Manejo presupuestos, proyecto de aprendizaje </h2>
            <h3 style="text-align: center; color: darkviolet">ASP NET.CORE y BOOTSTRAP</h3>
            <h5 style="text-align: center;">
                Dentro de esta aplicacion
                podras manejar tus finanzas</h5>
        </div>
    </div>
    
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
